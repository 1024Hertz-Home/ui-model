{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@ui-model/angular": [
        "dist/ui-model/angular",
        "projects/ui-model/angular"
      ],
      "@ui-model/angular-bootstrap": [
        "dist/ui-model/angular-bootstrap",
        "projects/ui-model/angular-bootstrap"
      ],
      "@ui-model/angular-markdown": [
        "dist/ui-model/angular-markdown",
        "projects/ui-model/angular-markdown"
      ],
      "@ui-model/chart": [
        "dist/ui-model/chart",
        "projects/ui-model/chart"
      ],
      "@ui-model/core": [
        "dist/ui-model/core",
        "projects/ui-model/core"
      ],
      "@ui-model/common": [
        "dist/ui-model/common",
        "projects/ui-model/common"
      ],
      "@ui-model/angular-d3": [
        "dist/ui-model/angular-d3",
        "projects/ui-model/angular-d3"
      ],
      "@ui-model/angular-svg": [
        "dist/ui-model/angular-svg",
        "projects/ui-model/angular-svg"
      ]
    }
  }
}
