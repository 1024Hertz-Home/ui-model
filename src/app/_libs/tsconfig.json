{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "es2015", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "outDir": "../../../publish/",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true,
    "stripInternal": false,
    "baseUrl": "",
    "typeRoots": [
      "../../../node_modules/@types"
    ],
    "types": [
      "rxjs"
    ],
    "paths": {
      "ui-model": ["./ui-model"],
      "ui-model-angular": ["./ui-model-angular"]
    }
  },
  "files": [
    "ui-model/index.ts",
    "ui-model-angular/index.ts",
    "ui-model-angular-bootstrap/index.ts"
  ],
  "exclude": ["**/*.spec.ts"],
  "angularCompilerOptions": {
    "genDir": "../../../dist",
    "skipTemplateCodegen": true,
    "debug": true
  }
}
