{
  "name": "ui-model",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 0 --open",
    "start-cn": "ng serve --aot --i18n-file=src/i18n/messages.zh.xlf --locale=zh --i18n-format=xlf --port 0 --open",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "test": "ng test -sr",
    "test-dev": "ng test --browsers=Chrome",
    "build": "ng build && cp -r ./src/app dist/",
    "build-cn": "ng build --prod --aot --i18n-file=src/i18n/messages.zh.xlf --locale=zh --i18n-format=xlf",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build-libs": "rm -fr ./publish && mkdir ./publish && cp -r ./src/app/@ui-model/* ./publish && ngc -p src/app/@ui-model/tsconfig.libs.json",
    "publish": "./publish.sh",
    "i18n": "ng xi18n --output-path src/i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^1.3.0-beta.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular-in-memory-web-api": "0.3.2",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3": "^4.7.3",
    "font-awesome": "^4.7.0",
    "geojson": "^0.4.1",
    "highlight.js": "^9.10.0",
    "lodash": "^4.17.4",
    "markdown-it": "^8.3.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-emoji": "^1.3.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.3.2",
    "moment": "^2.16.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@types/core-js": "^0.9.36",
    "@types/d3": "^4.7.0",
    "@types/geojson": "^1.0.2",
    "@types/highlight.js": "^9.1.9",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.52",
    "@types/markdown-it": "0.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
