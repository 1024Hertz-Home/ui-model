  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "test": "ng test",
    "build": "ng build --prod && cp -r ./src dist/",
    "build-cn": "ng build --aot --i18n-file=src/i18n/messages.zh.xlf --locale=zh --i18n-format=xlf",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build-libs": "rm -fr ./publish && mkdir ./publish && cp -r ./src/app/@ui-model/* ./publish && ngc -p src/app/@ui-model/tsconfig.json",
    "publish": "./publish.sh",
    "i18n": "ng xi18n --output-path src/i18n"
  },
  "private": true,
  "dependencies": {
